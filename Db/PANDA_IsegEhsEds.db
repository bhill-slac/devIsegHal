# ### macros: subsys      PANDA subsystem      (e.g. FEMC)       ### #
# ###         dev         detector subtype     (e.g. APD)        ### #
# ###         sector      sector inside subsys (e.g. Q1:X4:Y2:F) ### #
# ###         modsector   sector of HV module                    ### #
# ###         crate       sector of HV crate modules are in      ### #
# ###         CAN_LINE    isegHAL CAN line ID                    ### #
# ###         MODULE_ID   isegHAL module address                 ### #
# ###         CHANNEL_ID  isegHAL HV channel address             ### #
######################################################################

##############################
# ### Module item values ### #
##############################

record( mbbiDirect, "PANDA:$(subsys):HV:$(modsector):ModStatus" ) {
  field( DTYP, "isegHAL" )
  field( DISV, "0" )
  field( SDIS, "PANDA:$(subsys):HVCRATE:$(crate):PowerOn.VAL NPP MS" )
  field( INP,  "@${CAN_LINE}.${MODULE_ID}.Status" )
  field( FLNK, "PANDA:$(subsys):HV:$(modsector):ModEventStatus" )
  field( TSE,  "-2" )
}

record( mbbiDirect, "PANDA:$(subsys):HV:$(modsector):ModEventStatus" ) {
  field( DTYP, "isegHAL" )
  field( DISV, "0" )
  field( SDIS, "PANDA:$(subsys):HVCRATE:$(crate):PowerOn.VAL NPP MS" )
  field( INP,  "@${CAN_LINE}.${MODULE_ID}.EventStatus" )
  field( FLNK, "PANDA:$(subsys):HV:$(modsector):Temperature" )
  field( TSE,  "-2" )
}

