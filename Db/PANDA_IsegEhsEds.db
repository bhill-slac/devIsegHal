record( ai, "PANDA:$(subsys):HV:$(modsector):Temperature" ) {
  field( DTYP, "isegHAL" )
  field( DISV, "0" )
  field( SDIS, "PANDA:$(subsys):HVCRATE:$(crate):PowerOn.VAL NPP MS" )
  field( INP,  "@${CAN_LINE}.${MODULE_ID}.Temperature" )
  # display parameters
  field( EGU,  "degC" )
  field( PREC, "2" )
  # Alarm parameters
  field( HIHI, "50" )
  field( HIGH, "40" )
  field( LOW,  "0" )
  field( LOLO, "0" )
  field( HHSV, "MAJOR" )
  field( HSV,  "MINOR" )
  field( LSV,  "NO_ALARM" )
  field( LLSV, "NO_ALARM" )
  # thresholds
  field( ADEL, "0.5" )
  field( MDEL, "0" )
  field( TSE,  "-2" )
}

